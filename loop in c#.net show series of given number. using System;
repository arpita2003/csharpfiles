using System;
namespace loop
{ 

publicclassProgram
    {
publicstaticvoid Main()
        {	
int i,n;
			Console.WriteLine("Enter a number:-");
		    n=Convert.ToInt32(Console.ReadLine());
		Console.WriteLine("seris of 1 to 10 in for loop:-");
		for(i=1;i<=n;i++)
		    {
			Console.WriteLine(i); 
		    }		Console.ReadLine();   
        }     }   }
